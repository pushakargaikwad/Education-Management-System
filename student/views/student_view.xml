<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record id="view_student_form" model="ir.ui.view">
			<field name="name">Student Form</field>
			<field name="model">pg.student</field>
			<field name="priority" eval="8" />
			<field name="arch" type="xml">
				<form string="Student" version="7.0">
					<header>
						<span>
                            <field name="name"/>
                        </span>
					</header>
					

				</form>
			</field>
		</record>
        		<record id="view_student_tree" model="ir.ui.view">
			<field name="name">Student tree</field>
			<field name="model">pg.student</field>
			<field name="priority" eval="8" />
			<field name="arch" type="xml">
				<tree string="Student">
					
					<field name="name" />

				</tree>
			</field>

            		
		</record>

        <record model="ir.actions.act_window" id="act_open_student_view">
			<field name="name">Students</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">pg.student</field>
			<field name="binding_view_types">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[]</field>
			<field name="context">{}</field>
		</record>
        
        		<record model="ir.actions.act_window.view" id="act_open_student_view_tree">
			<field name="act_window_id" ref="act_open_student_view" />
			<field name="sequence" eval="10" />
			<field name="view_mode">tree</field>
			<field name="view_id" ref="view_student_tree" />
		</record>
		

		<record model="ir.actions.act_window.view" id="act_open_student_view_form">
			<field name="act_window_id" ref="act_open_student_view" />
			<field name="sequence" eval="20" />
			<field name="view_mode">form</field>
			<field name="view_id" ref="view_student_form" />
		</record>

        
        <menuitem id="menu_student" name="Students"
		     sequence="0" groups="base.group_user"/>
        <menuitem id="submenu_student" name="Student" sequence="1" parent="menu_student" action="act_open_student_view" groups="base.group_user"/>


		<menuitem id="menu_action_student" name="Students"
			parent="submenu_student" sequence="20"
			action="act_open_student_view" groups="base.group_user" />

        

        
        
        
	</data>
</openerp>
